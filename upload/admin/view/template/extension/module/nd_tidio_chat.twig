{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <a onclick="$('#form').submit();" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><span><i class="fa fa-save"></i></span></a>
        <a href="{{ back }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
      </div>
      <h1><i class="fa fa-comments" style="color:#00b6ec"></i> {{ heading_title }} <sub class="h6 text-info">by Nexa Domain</sub></h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
          <li class="breadcrumb-item"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="container-fluid">
    {% if error_warning %}
    <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    <div class="panel panel-default">
      <div class="panel-heading"><i class="fa fa-pencil"></i> {{ text_edit }}</div>
      <div class="panel-body">
        <div class="alert alert-info" role="alert" style="display: flex; justify-content: space-between;">
          <a href="https://rcert.in/oc_tidio_install.html" target="_blank">
            <i class="fa fa-info-circle"></i> {{ text_set_up_tidio }}
          </a>
          <a href="https://www.kqzyfj.com/click-100540529-15182839" target="_blank">
            <span class="float-end"><i class="fa fa-comments"></i> {{ text_sign_up_tidio }}</span>
          </a>
        </div>
        <form id="form" action="{{ save }}" method="post" class="form-horizontal">
          <div class="form-group">
            <label class="col-sm-2 control-label" for="input-status">{{ entry_status }}</label>
            <div class="col-sm-10">
              <select name="module_nd_tidio_chat_status" id="input-status" class="form-control">
                {% if tidio_chat_status %}
                  <option value="1" selected="selected">{{ text_enabled }}</option>
                  <option value="0">{{ text_disabled }}</option>
                {% else %}
                  <option value="1">{{ text_enabled }}</option>
                  <option value="0" selected="selected">{{ text_disabled }}</option>
                {% endif %}
              </select>
            </div>
          </div>
          <div class="form-group">
            <label for="input-script-code" class="col-sm-2 control-label">{{ entry_script_code }}</label>
            <div class="col-sm-10">
              <textarea name="module_nd_tidio_chat_script_code" rows="5" placeholder="{{ help_script_code_1 }}" id="input-tidio-script-code" class="form-control">{{ tidio_chat_script_code }}</textarea>
              <div class="form-text"><a href="https://www.kqzyfj.com/click-100540529-15182839" target="_blank">{{ help_script_code_2 }}</a></div>
            </div>
          </div>
        </form>
      </div>
    </div>
    <hr>
    <div class="text-center">
      <a href="https://www.jdoqocy.com/click-100540529-15200289" target="_blank">
        <picture>
          <source media="(min-width: 1290px)" srcset="https://www.tqlkg.com/image-100540529-15182994">
          <source media="(min-width: 768px)" srcset="https://www.tqlkg.com/image-100540529-15182993">
          <img src="https://www.tqlkg.com/image-100540529-15182944" alt="Tidio Banner" loading="lazy">
        </picture>
      </a>
    </div>
    <hr>
    <h4 class="text-center"><a href="https://www.jdoqocy.com/click-100540529-15182806" target="_blank">{{ pricing_subtitle }}</a></h4>
  </div>
</div>
{{ footer }}
